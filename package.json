{
  "type": "module",
  "name": "api-static",
  "description": "The API service for USThing static resources or static data.",
  "version": "0.4.0",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn build && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 tsx --test --experimental-test-coverage test/**/*.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "fastify start -l info dist/src/app.js --options",
    "dev": "yarn run build && concurrently -k -p '[{name}]' -n TypeScript,App -c yellow.bold,cyan.bold yarn:watch yarn:dev:start",
    "dev:start": "fastify start --ignore-watch='.ts$' -w -l info -P dist/src/app.js --options",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "keywords": [],
  "author": "Flandia",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/bearer-auth": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@fastify/type-provider-typebox": "^4.1.0",
    "@sinclair/typebox": "^0.33.17",
    "date-fns": "^4.1.0",
    "fastify": "^4.28.1",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.5.1",
    "openid-client": "^5.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.8.7",
    "c8": "^10.1.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fastify-tsconfig": "^2.0.0",
    "globals": "^15.11.0",
    "prettier": "3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  },
  "packageManager": "yarn@4.4.0"
}
